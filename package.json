    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "hookem": "^2.0.1",
    "jest": "^27.4.7",
    "lint-staged": "^12.2.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "typescript": "^4.5.4"
