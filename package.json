    "@docusaurus/plugin-content-pages": "2.0.0-beta.14",
    "@docusaurus/plugin-debug": "2.0.0-beta.14",
    "@docusaurus/plugin-sitemap": "2.0.0-beta.14",
    "@docusaurus/theme-search-algolia": "2.0.0-beta.14",
