{
  "name": "@4c/docusaurus-preset",
  "version": "0.2.5",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/4Catalyzer/docusaurus-preset.git"
  },
  "author": "4Catalyzer",
  "license": "MIT",
  "scripts": {
    "build": "4c build src",
    "format": "4c format src './*'",
    "lint": "4c lint src './*'",
    "prepublishOnly": "yarn run build",
    "release": "4c release",
    "test": "yarn lint && yarn typecheck",
    "typecheck": "tsc --noEmit"
  },
